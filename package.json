{
  "name": "dungeon-crawler",
  "version": "1.0.0",
  "description": "A dungeon crawler game developed in Phaser3",
  "scripts": {
    "start": "parcel src/index.html -p 8000",
    "build": "parcel build src/index.html --out-dir dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "supertommy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shutt90/dungeon-crawler.git"
  },
  "homepage": "https://github.com/shutt90/dungeon-crawler.git",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "minimist": ">=1.2.2",
    "parcel-plugin-clean-easy": "^1.0.2",
    "parcel-plugin-static-files-copy": "^2.4.3"
  },
  "dependencies": {
    "phaser": "^3.55.2"
  },
  "parcelCleanPaths": [
    "dist"
  ],
  "staticFiles": {
    "staticPath": "public",
    "watcherGlob": "**"
